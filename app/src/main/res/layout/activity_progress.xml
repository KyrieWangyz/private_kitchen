<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:layout_height="match_parent"
    android:layout_width="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android">



    <com.youdu.ButtonTest.CustomFourImageView
        android:layout_width="match_parent"
        android:layout_height="290dp" />
    <ImageView
        android:id="@+id/back_view"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginTop="22dp"
        android:layout_marginLeft="22dp"
        android:background="@drawable/comment_back_normal"
        />


    <android.support.v4.view.ViewPager
        android:id="@+id/viewpage"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"></android.support.v4.view.ViewPager>

    <ImageView
        android:id="@+id/personalcenter_icon_set"
        android:src="@mipmap/personalcenter_icon_set"
        android:layout_alignParentRight="true"
        android:layout_marginTop="22dp"
        android:layout_marginRight="22dp"
        android:layout_width="45dp"
        android:layout_height="45dp" />




</RelativeLayout>