<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 必须使用此id,drawable为 未选中 状态的图片 -->
    <item
        android:id="@+id/background"
        android:drawable="@drawable/drawable_home_btn"/>
    <!-- 必须使用此id,drawable为 半选中 状态的图片 -->
    <item
        android:id="@+id/secondaryProgress"
        android:drawable="@drawable/drawable_home_btn"/>
    <!-- 必须使用此id,drawable为 全选中 状态的图片 -->
    <item
        android:id="@+id/progress"
        android:drawable="@drawable/drawable_message_btn"/>
</layer-list>